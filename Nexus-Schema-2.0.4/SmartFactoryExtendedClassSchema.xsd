			 <element ref="sfas:groupId" minOccurs="0"/>
			 <element ref="sfas:documentList" minOccurs="0" maxOccurs="unbounded"/>
			 <element ref="sfas:alternativeIDList" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		</extension>
	 </complexContent>
	</complexType>
	<complexType name="Workflow">
	 <annotation>
